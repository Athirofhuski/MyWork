        <ImageButton
            android:id="@+id/seg_p"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/b3"
            app:srcCompat="@drawable/settings_icon" />
