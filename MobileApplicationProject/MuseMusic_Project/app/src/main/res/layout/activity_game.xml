            android:id="@+id/sg_p"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/b3"
            app:srcCompat="@drawable/shopping_icon" />

        <ImageButton
            android:id="@+id/gg_p"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/b2"
